{
  "name": "e-commerce-db",
  "version": "1.0.0",
  "description": "Database supporting e-commerce activities",
  "author": {
    "name": "Jim Barrows",
    "email": "Jim.Barrows@gmail.com"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "docker build --rm --tag erpmicroservices/e-commerce-db .",
    "db_admin": "docker run --detach --publish 5050:5050 --name e-commerce-db-pgadmin --link e-commerce-db:e-commerce-db thajeztah/pgadmin4",
    "push": "docker push erpmicroservices/e-commerce-db:latest",
    "start": "docker run --publish 5432:5432 --name e-commerce-db erpmicroservices/e-commerce-db",
    "test": "cucumber-js --tags @database --compiler es6:babel-core/register  -r ./features ../e-commerce-features ",
    "test:one": "cucumber-js --name \"I can create a function type\" --compiler es6:babel-core/register  -r ./features ../e-commerce-features"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ErpMicroServices/e-commerce-db.git"
  },
  "bugs": {
    "url": "https://github.com/ErpMicroServices/e-commerce-db/issues"
  },
  "homepage": "https://github.com/ErpMicroServices/e-commerce-db#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "bluebird": "^3.5.0",
    "chai": "^3.5.0",
    "cucumber": "^2.0.0-rc.7",
    "jsonwebtoken": "^7.3.0",
    "pg-promise": "^5.6.2",
    "uuid": "^3.0.1"
  }
}
